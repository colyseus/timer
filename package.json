    "@types/node": "^22.8.7",
    "prettier": "^3.3.3",
    "tsup": "^8.3.5",
    "typescript": "^5.3.3",
    "vitest": "^2.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/colyseus/timer.git"
  },
  "keywords": [
