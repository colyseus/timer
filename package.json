  },
  "homepage": "https://github.com/colyseus/timer#readme",
  "dependencies": {
    "@colyseus/clock": "^1.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/assert": "^1.5.10",
    "@types/mocha": "^10.0.6",
    "@types/node": "^22.13.4",
    "assert": "^2.1.0",
    "esbuild": "^0.23.0",
    "fast-glob": "^3.3.2",
    "mocha": "^10.3.0",
    "rimraf": "^5.0.5",
    "tsx": "^4.19.3",
    "typescript": "^5.3.3"
  }
